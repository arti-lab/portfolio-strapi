{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "githubUrl": {
      "type": "string"
    },
    "liveUrl": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "stack": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::stack.stack",
      "inversedBy": "projects"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Web_App",
        "Mobile",
        "Full_Stack"
      ]
    }
  }
}
